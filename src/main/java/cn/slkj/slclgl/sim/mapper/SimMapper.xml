<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.slkj.slclgl.sim.mapper.SimMapper">
	<select id="getAll" resultType="Sim" parameterType="hashmap">
		SELECT * FROM TB_SIM  WHERE TYPE =#{type} LIMIT  #{startPage},#{endPage}
	</select>
	
	<select id="getAllCount" resultType="int" parameterType="hashmap">
		SELECT COUNT(1) FROM TB_SIM WHERE TYPE =#{type}
	</select>
	
	<insert id="insert" parameterType="Module">
		INSERT INTO TB_SIM
		(TYPE,CARDTYPE,LISTNUM,TELNUM,STATE,INTIME,OUTTIME,KKTIME,FHTIME,BEIZHU,LYR,GYS,lrr)
		VALUES(#{type},#{cardType},#{listnum},#{telnum},#{state},#{intime},#{outtime},#{kktime},#{fhtime},#{beizhu},
		#{lyr},#{gys},#{lrr})
	</insert>
</mapper>